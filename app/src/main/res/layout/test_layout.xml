                    <RadioButton
                        android:id="@+id/q_nine_c"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="(null)" />

                    <RadioButton
                        android:id="@+id/q_nine_d"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="Compiler Error" />
                </RadioGroup>
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="#fff"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_margin="10dp"
                    android:text="@string/question_ten"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="#880E4F" />


                <RadioGroup
                    android:id="@+id/radio_group_ten"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp">


                    <RadioButton
                        android:id="@+id/q_ten_a"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="0 1 2" />

                    <RadioButton
                        android:id="@+id/q_ten_b"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="0 1 2 3" />


                    <RadioButton
                        android:id="@+id/q_ten_c"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="1 2 3" />

                    <RadioButton
                        android:id="@+id/q_ten_d"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="Compiler Error" />
                </RadioGroup>

                <Button
                    android:id="@+id/test_submit_btn"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="8dp"
                    android:background="#3E2723"
                    android:text="Submit"
                    android:textColor="#fff" />
